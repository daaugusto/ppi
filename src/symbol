#ifndef __SYMBOL_H
#define __SYMBOL_H

#define TERMINAL_MIN 10000
#define ATTRIBUTE_MIN 20000

/** ****************************************************************** **/
/** ***************************** TYPES ****************************** **/
/** ****************************************************************** **/

typedef enum { NT_INICIAL, NT_RAND, NT_BIN_OP, NT_UN_OP, NT_ATRIBUTO, NT_CONST_NUMERICO, T_IF_THEN_ELSE = TERMINAL_MIN, T_AND, T_OR, T_NOT, T_GREATER, T_LESS, T_EQUAL, T_ADD, T_SUB, T_MULT, T_DIV, T_MEAN, T_MAX, T_MIN, T_MOD, T_ABS, T_SQRT, T_POW2, T_POW3, T_NEG, T_CONST, T_1P, T_2P, T_3P, T_4P, T_ATTRIBUTE, T_ATTR1 = ATTRIBUTE_MIN, T_ATTR2, T_ATTR3, T_ATTR4, T_ATTR5, T_ATTR6, T_ATTR7, T_ATTR8, T_ATTR9, T_ATTR10 } Symbol;
#endif
